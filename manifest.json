{
    "manifest_version": 3,
    "name": "Financial Manager",
    "version": "1.0",
    "description": "Manage your finances directly from a Chrome extension popup.",
    "action": {
      "default_popup": "FinancialManager.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "permissions": [],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  